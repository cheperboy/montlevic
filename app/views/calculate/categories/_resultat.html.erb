	<tr>
		<td class=col1><%= link_to 'Resultat', "javascript: toggleLevel2('resultat');" %></td>
		<td class=col2></td>
		<td class=col3></td>
		<td class="saison"><b><%= @test.res.get_other_line_for_saison(@test.sid, :resultats, :benef, :ha).display %></b></td>
		<td class="saison"><b><%= @test.res.get_other_line_for_saison(@test.sid, :resultats, :benef, :total).display %></b></td>	
		<% rang = 0 %>
		<% @colonnes.each do |colonne| %>
			<% rang += 1 %>
			<% cout_ha = @test.res.get_other_line(@test.sid, @test.c, colonne.id, :resultats, :benef, :ha).display %>
			<% cout_total = @test.res.get_other_line(@test.sid, @test.c, colonne.id, :resultats, :benef, :total).display %>
			<td class="<%= odd(rang) %>_subtotal"><b><%= cout_ha %></b></td>
			<td class="<%= odd(rang) %>_subtotal"><b><%= cout_total %></b></td>
		<% end %>
	</tr>






