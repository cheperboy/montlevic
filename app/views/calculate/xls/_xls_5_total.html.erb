	<tr>
		<% td_number(@colonnes.length, @show[:cout_ha], @show[:cout_total]).times do %>
				<td></td>
		<% end %>
	</tr>	
<% unless (@colonnes.size == 0) %>
<!-- Total  -->
	<tr class="total">
		<td class="left"><b>Total Charges</b></td>
		<td></td>
		<td></td>
		<td></td>
		<td><%= pretty(@test.Tcols[:charges][:ha]) %></td>
		<td class="td-total"><%= pretty(@test.Tcols[:charges][:total]) %></td>	
	<% @colonnes.each do |colonne| %>
		<% cout_ha = pretty(@test.Tcols[colonne.id][:charges][:ha][:sum]) %>
		<% cout_total = pretty(@test.Tcols[colonne.id][:charges][:total][:sum]) %>
		<% td_class = odd(@test.Tcols[colonne.id][:rang]) %>
		<% if @show[:cout_ha] %>
			<td class="<%= td_class %>"><%=h cout_ha %></td>
		<% end %>
		<% if @show[:cout_total] %>
			<td class="<%= td_class %>"><%=h cout_total %></td>
		<% end %>	<% end %>
	</tr>
<% end %>	

