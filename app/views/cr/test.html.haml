- title "CR"

%table.table_print
  = render(:partial => "calculate/categories/p1_head")
  %tr
    %td.col1= link_to 'Type', "javascript: toggleAllLevel2();"
    %td.col2= link_to 'Categorie', "javascript: toggleAllLevel3();", :name => 'hide'
    %td.col3 Facture
    %td.saison{:colspan => "2"}
    - rang = 0
    - @colonnes.each do |colonne|
      - rang += 1
      %td{:class => "color_culture_#{colonne.code}"}
      %td{:class => "color_culture_#{colonne.code}"}
  %tr
    %td.col1
    %td.col2
    %td.col3
    %td{:class => "col4.1"}
    %td{:class => "col4.2"}
    - @colonnes.each do |colonne|
      %td
      %td
  = render(:partial => "cr/test/c11_factures")
/  = render(:partial => "cr/test/c31_ventes")
/  = render(:partial => "cr/test/resultat")
